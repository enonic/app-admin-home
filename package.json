{
  "name": "app-main",
  "version": "1.0.0",
  "description": "Enonic Main App",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/enonic/app-admin-home"
  },
  "scripts": {
    "build": "concurrently -c auto -g --timings npm:build:main:*",
    "build:main:assets": "webpack --color --stats-error-details",
    "build:main:server": "tsup -d build/resources/main",
    "build:main:static": "tsup -d build/resources/main/static",
    "check:types:server": "tsc --pretty --noEmit -p src/main/resources/tsconfig.json",
    "clean": "del build",
    "lint": "eslint **/*.ts --quiet --cache",
    "fix": "eslint --fix **/*.ts --cache",
    "postupgrade:gradle-wrapper": "enonic project clean",
    "upgrade:gradle-wrapper": "enonic project gradle wrapper --gradle-version latest"
  },
  "dependencies": {
    "@enonic/lib-admin-ui": "file:./.xp/dev/lib-admin-ui",
    "jquery": "^3.7.1",
    "q": "^1.5.1"
  },
  "devDependencies": {
    "@enonic-types/global": "^7.13.2",
    "@enonic-types/lib-admin": "^7.13.2",
    "@enonic-types/lib-auth": "^7.13.2",
    "@enonic-types/lib-i18n": "^7.13.2",
    "@enonic-types/lib-io": "^7.13.2",
    "@enonic-types/lib-portal": "^7.13.2",
    "@enonic/esbuild-plugin-copy-with-hash": "^0.0.1",
    "@enonic/eslint-config": "^1.2.0",
    "@swc/core": "^1.3.93",
    "@enonic/tsup-plugin-manifest": "^0.0.1",
    "@types/jquery": "^3.5.22",
    "@types/q": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.16",
    "browserslist-config-enonic": "^1.0.8",
    "circular-dependency-plugin": "^5.2.2",
    "concurrently": "^8.2.1",
    "css-loader": "^6.8.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^6.0.1",
    "del-cli": "^5.1.0",
    "enonic-admin-artifacts": "^1.8.5",
    "esbuild-plugin-external-global": "^1.0.1",
    "eslint": "^8.51.0",
    "glob": "^10.3.4",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "postcss-normalize": "^10.0.1",
    "sharp": "^0.32.6",
    "source-map-loader": "^4.0.1",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
      "node": ">= 18.17.1",
      "npm": ">= 9.6.7"
  }
}
